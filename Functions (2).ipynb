    "def reverse(str1):\n",
    "    reverse_str1 = ''\n",
    "    \n",
    "    index = len(str1)\n",
    "    \n",
    "    while index > 0:\n",
    "        reverse_str1 += str1[index - 1]\n",
    "        index = index - 1\n",
    "    return reverse_str1\n",
    "print(reverse(\"1234abcd\"))"
   ]
